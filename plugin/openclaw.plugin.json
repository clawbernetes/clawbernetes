{
  "id": "clawbernetes",
  "name": "Clawbernetes",
  "description": "AI-native GPU orchestration tools for OpenClaw",
  "version": "0.1.0",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "bridgePath": {
        "type": "string",
        "description": "Path to the claw-bridge binary"
      },
      "defaultCluster": {
        "type": "string",
        "description": "Default cluster name",
        "default": "default"
      }
    },
    "required": ["bridgePath"]
  },
  "uiHints": {
    "bridgePath": {
      "label": "Bridge Binary Path",
      "placeholder": "/path/to/claw-bridge"
    },
    "defaultCluster": {
      "label": "Default Cluster",
      "placeholder": "default"
    }
  },
  "skills": [
    "../skills/canary-release",
    "../skills/blue-green-deploy",
    "../skills/gpu-diagnose",
    "../skills/training-job",
    "../skills/spot-migration",
    "../skills/auto-heal",
    "../skills/cost-optimize",
    "../skills/incident-response"
  ]
}
